{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "ie >= 8"
        ]
      },
      "useBuiltIns": "usage",
      "modules": false
    }],
    "./lib/babel/preset-stage"
  ],
  "plugins": [
    "@babel/plugin-transform-reserved-words"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "./lib/babel/preset-stage"
      ],
      "plugins": [
        "array-includes"
      ]
    }
  }
}